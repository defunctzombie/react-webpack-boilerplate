{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS projects with hot code reloading with Express",
  "scripts": {
    "start": "node bin/www",
    "dev": "NODE_ENV=development node-dev bin/www",
    "test": "",
    "build": "npm run clean && NODE_ENV=production webpack --optimize-minimize",
    "lint": "eslint --ext jsx --ext js .",
    "clean": "rm -rf dist",
    "git-hooks": "cd .git/hooks && ln -sf ../../bin/pre-commit ./"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.3.26",
    "babel-preset-es2015-node4": "2.0.2",
    "book": "1.3.1",
    "bookrc": "0.0.1",
    "debug": "2.2.0",
    "express": "4.13.3",
    "hbs": "4.0.0",
    "httperrors": "1.1.0",
    "localenv": "0.2.2",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "safestart": "0.8.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.2.0",
    "babel-loader": "6.2.0",
    "babel-plugin-react-transform": "2.0.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "optionalDependencies": {
    "babel-eslint": "5.0.0-beta6",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "2.1.1",
    "eslint-plugin-react": "3.12.0",
    "eslint-plugin-babel": "3.0.0",
    "node-dev": "2.7.1"
  }
}
